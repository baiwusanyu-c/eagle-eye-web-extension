{
  "name": "eagle-eye-web-extension",
  "description": "基于谷歌内核的态势感知浏览器插件扩展",
  "version": "1.0.0",
  "public": true,
  "scripts": {
    "server:dev": "pnpm run server:manifest && run-p server:dev:*",
    "server:dev:web": "vue-tsc --noEmit && vite build --watch --mode=development",
    "server:dev:background": "vue-tsc --noEmit && vite build --watch --mode=development --config vite-background.config.ts",
    "server:dev:content": "vite build --watch --mode=development --config vite-content.config.ts",
    "server:stage": "pnpm run server:manifest && run-p server:stage:*",
    "server:stage:web": "vue-tsc --noEmit && vite build --watch --mode=staging",
    "server:stage:background": "vue-tsc --noEmit && vite build --watch --mode=staging --config vite-background.config.ts",
    "server:stage:content": "vite build --watch --mode=staging --config vite-content.config.ts",
    "server:prod": "pnpm run server:manifest && run-p server:stage:*",
    "server:prod:web": "vue-tsc --noEmit && vite build --watch --mode=development",
    "server:prod:background": "vue-tsc --noEmit && vite build --watch --mode=development --config vite-background.config.ts",
    "server:prod:content": "vite build --watch --mode=development --config vite-content.config.ts",
    "server:manifest": "esno ./scripts/manifest.ts",
    "build:dev": "pnpm run server:manifest && run-p server:dev:*",
    "build:stage": "pnpm run server:manifest && run-p server:stage:*",
    "build:prod": "pnpm run server:manifest && run-p server:prod:*",
    "lint:ts": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:ts-fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx ",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.5",
    "@vueuse/core": "^8.3.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.33",
    "@types/qs": "^6.9.7",
    "axios": "^0.24.0",
    "element-plus": "2.1.8",
    "qs": "^6.10.1",
    "vite-plugin-svg-transform-script": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/chrome": "^0.0.173",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.5",
    "@types/webextension-polyfill": "^0.8.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "esno": "^0.13.0",
    "fs-extra": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "semver": "^7.3.5",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.34.9",
    "webextension-polyfill": "^0.9.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-unicorn": "^41.0.1",
    "jsonc-eslint-parser": "^2.1.0",
    "yaml-eslint-parser": "^0.5.0",
    "husky": "^7.0.4",
    "sass": "^1.49.0",
    "prettier": "2.5.1",
    "pretty-quick": "v3.1.3 "
  }
}
