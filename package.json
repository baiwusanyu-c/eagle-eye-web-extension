{
  "name": "eagle-eye-web-extension",
  "description": "基于谷歌内核的态势感知浏览器插件扩展",
  "version": "1.0.3",
  "public": true,
  "scripts": {
    "chrome:dev:popup": "esno  ./scripts/browser.js chrome && vite build --watch --mode=development",
    "chrome:dev:background": "esno  ./scripts/browser.js chrome && vite build --watch --mode=development --config vite-background.config.ts",
    "chrome:dev:content": "esno  ./scripts/browser.js chrome && vite build --watch --mode=development --config vite-content.config.ts",
    "chrome:manifest": "esno ./scripts/manifest-chrome.js",
    "safari:dev:popup": "esno  ./scripts/browser.js safari && vite build --watch --mode=development",
    "safari:prod:background": "esno  ./scripts/browser.js safari && vite build --watch --mode=production --config vite-background.config.ts",
    "safari:prod:content": "esno  ./scripts/browser.js safari && vite build --watch --mode=production --config vite-content.config.ts",
    "safari:manifest": "esno ./scripts/manifest-safari.js",
    "build": "gulp -f scripts/gulpfile.esm.js",
    "clean": "rimraf dist",
    "lint:ts": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:ts-fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx ",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "qs": "^6.10.1",
    "vite-plugin-svg-transform-script": "^1.1.0",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/chrome": "^0.0.173",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.5",
    "@types/qs": "^6.9.7",
    "@types/webextension-polyfill": "^0.8.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "eslint-plugin-vue": "^8.5.0",
    "esno": "^0.13.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "jsonc-eslint-parser": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "pretty-quick": "v3.1.3 ",
    "sass": "^1.49.0",
    "semver": "^7.3.5",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.34.9",
    "webextension-polyfill": "^0.9.0",
    "yaml-eslint-parser": "^0.5.0",
    "rimraf": "^3.0.2"
  }
}
